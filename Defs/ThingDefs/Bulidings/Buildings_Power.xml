					<li>
						<key>1</key>
						<value>
							<requireMaterial>true</requireMaterial>
							<materials>
								<li>
									<item>SSR_SpecialSteel</item>
									<amount>10</amount>
								</li>
								<li>
									<item>ComponentIndustrial</item>
									<amount>3</amount>
								</li>
							</materials>
							<workAmt>100</workAmt>
							<requirePawnWork>true</requirePawnWork>
							<graphicData>
								<texPath>Things/Building/Power/ThermalPowerPlant/ThermalPowerPlant</texPath>
								<drawSize>(5,5)</drawSize>
							</graphicData>
							<requireResearch>SSRWasteGasTreatmentTechnology</requireResearch>
							<showUpgradeGizmo>true</showUpgradeGizmo>
						</value>
					</li>
				</levels>
			</li>
			<li Class="SSR.Effect.CompProperties_SinglePerfabDrawer">
				<leveledPerfabPaths>
					<li>Assets/SSR/Perfab/ThermalPower.prefab</li>
				</leveledPerfabPaths>
			</li>
		</comps>
		<terrainAffordanceNeeded>Heavy</terrainAffordanceNeeded>
		<uiOrder>2100</uiOrder>
		<designationHotKey>Misc4</designationHotKey>
		<constructEffect>ConstructMetal</constructEffect>
		<researchPrerequisites>
			<li>SSRBasicPowerTechnology</li>
		</researchPrerequisites>
		<constructionSkillPrerequisite>6</constructionSkillPrerequisite>
