			<texPath>Things/Building/Power/PowerConduit/PowerConduit</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<linkType>Transmitter</linkType>
			<linkFlags>
				<li>PowerConduit</li>
			</linkFlags>
			<damageData>
				<rect>(0,0.35,1,0.3)</rect>
			</damageData>
		</graphicData>
		<uiIconPath>Things/Building/Power/PowerConduit/PowerConduit-Icon</uiIconPath>
		<uiIconScale>0.7</uiIconScale>
		<building>
			<blueprintGraphicData>
				<texPath>Things/Building/Linked/PowerConduit_Blueprint_Atlas</texPath>
			</blueprintGraphicData>
		</building>
		<statBases>
			<WorkToBuild>70</WorkToBuild>
			<Flammability>0</Flammability>
			<Beauty>0</Beauty>
		</statBases>
		<useHitPoints>false</useHitPoints>
