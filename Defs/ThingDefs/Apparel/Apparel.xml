  <ThingDef ParentName="SSR_ApparelCustomBase">
    <defName>SSR_EF_LandForce_TrainingClothes</defName>
    <label>陆军作训服</label>
    <description>陆军作训服</description>
    <graphicData>
      <texPath>Things/Apparel/EF_LandForce_TrainingClothes/EF_LandForce_TrainingClothes</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <costList>
		  <SSR_Graphene>5</SSR_Graphene>
    </costList>
    <statBases>
      <MaxHitPoints>300</MaxHitPoints>
      <WorkToMake>1600</WorkToMake>
      <Mass>0.25</Mass>
      <ArmorRating_Sharp>0.2</ArmorRating_Sharp>
		  <ArmorRating_Blunt>0.2</ArmorRating_Blunt>
		  <ArmorRating_Heat>0.2</ArmorRating_Heat>
		  <Insulation_Cold>3</Insulation_Cold>
		  <Insulation_Heat>3</Insulation_Heat>
      <EquipDelay>0.25</EquipDelay>
    </statBases>
    <apparel>
      <bodyPartGroups>
        <li>Torso</li>
			  <li>Shoulders</li>
			  <li>Waist</li>
			  <li>Arms</li>
			  <li>Legs</li>
			  <li>Feet</li>
      </bodyPartGroups>
      <wornGraphicPath>Things/Apparel/EF_LandForce_TrainingClothes/EF_LandForce_TrainingClothes</wornGraphicPath>
      <layers>
        <li>OnSkin</li>
      </layers>
      <canBeDesiredForIdeo>false</canBeDesiredForIdeo>
    </apparel>
    <recipeMaker>
	  	<researchPrerequisite>EFClothing</researchPrerequisite>
		  <skillRequirements>
			  <Crafting>5</Crafting>
		  </skillRequirements>
    </recipeMaker>
  </ThingDef>

