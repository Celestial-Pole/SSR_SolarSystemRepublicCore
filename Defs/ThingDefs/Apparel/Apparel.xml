  <ThingDef ParentName="SSR_HatBase">
    <defName>SSR_EF_LandForce_TrainingClothes_helmet</defName>
    <label>陆军作训服帽子</label>
    <description>陆军作训服帽子</description>
    <graphicData>
      <texPath>Things/Apparel/EF_LandForce_TrainingClothes_helmet/EF_LandForce_TrainingClothes_helmet</texPath>
      <shaderType>CutoutComplex</shaderType>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <costList>
	    <SSR_Graphene>3</SSR_Graphene>
	  </costList>
    <statBases>
      <WorkToMake>900</WorkToMake>
	    <ArmorRating_Sharp>0.1</ArmorRating_Sharp>
      <ArmorRating_Blunt>0.1</ArmorRating_Blunt>
      <ArmorRating_Heat>0.1</ArmorRating_Heat>
      <Insulation_Cold>0.3</Insulation_Cold>
	    <Insulation_Heat>0.3</Insulation_Heat>
      <Mass>0.1</Mass>
      <EquipDelay>0.01</EquipDelay>
    </statBases>
	  <tickerType>Normal</tickerType>
    <apparel>
      <countsAsClothingForNudity>false</countsAsClothingForNudity>
      <bodyPartGroups>
        <li>UpperHead</li>
      </bodyPartGroups>
      <wornGraphicPath>Things/Apparel/EF_LandForce_TrainingClothes_helmet/EF_LandForce_TrainingClothes_helmet</wornGraphicPath>
      <useWornGraphicMask>true</useWornGraphicMask>
      <canBeDesiredForIdeo>false</canBeDesiredForIdeo>
	    <layers>
        <li>Overhead</li>
      </layers>
    </apparel>
	  <recipeMaker>
	  	<researchPrerequisite>EFClothing</researchPrerequisite>
		  <skillRequirements>
			  <Crafting>4</Crafting>
		  </skillRequirements>
    </recipeMaker>
	  <modExtensions>
		  <li Class="SolarSystemRepublicFramework.DefModExtension_DefendHediff">
  			<hediffs>
				<li>DirtInEyes</li>
				<li>GravelInEyes</li>
				<li>MudInEyes</li>
				<li>SandInEyes</li>
				<li>WaterInEyes</li>
  			</hediffs>
		  </li>
    </modExtensions>
  </ThingDef>

