			<CDUnit>tick</CDUnit>
			<targetMode>VerbSingle</targetMode>
			<allowPostEffect>false</allowPostEffect>
			<verb>
				<requireLineOfSight>false</requireLineOfSight>
				<verbClass>AKA_Ability.Verb_AbilityTargeting</verbClass>
				<defaultProjectile>Bullet_MiniTurret</defaultProjectile>
				<nonInterruptingSelfCast>false</nonInterruptingSelfCast>
				<warmupTime>0</warmupTime>
				<range>20</range>
				<soundCast>GunShotA</soundCast>
				<soundCastTail>GunTail_Light</soundCastTail>
				<muzzleFlashScale>0</muzzleFlashScale>
				<targetParams>
					<canTargetPawns>false</canTargetPawns>
					<canTargetLocations>true</canTargetLocations>
					<canTargetMechs>false</canTargetMechs>
					<canTargetBuildings>false</canTargetBuildings>
					<canTargetItems>false</canTargetItems>
				</targetParams>
			</verb>
			<compEffectList>
				<li Class="AKA_Ability.AE_Jump">
					<verbJump>
						<verbClass>Verb_Jump</verbClass>
						<warmupEffecter>JumpWarmupEffect</warmupEffecter>
						<flightEffecterDef>JumpFlightEffect</flightEffecterDef>
						<soundLanding>JumpPackLand</soundLanding>
					</verbJump>
				</li>
			</compEffectList>
			<typeSFX>heal</typeSFX>
