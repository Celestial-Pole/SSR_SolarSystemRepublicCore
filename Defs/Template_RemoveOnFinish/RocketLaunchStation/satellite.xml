	<ThingDef Class="FS_SSR.Satellite.LaunchStationDef" ParentName="BuildingBase" Name="SSR_LaunchStation">
		<defName>SSR_LaunchStation</defName>
		<label>火箭发射台</label>
		<description>火箭发射台</description>
		<thingClass>FS_SSR.Satellite.Building_LaunchStation</thingClass>
		<tickerType>Normal</tickerType>
		<graphicData>
			<texPath>Things/Building/Production/MaterialSynthesizer/MaterialSynthesizer</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<drawSize>(3,2)</drawSize>
			<damageData>
				<cornerTL>Damage/Corner</cornerTL>
				<cornerTR>Damage/Corner</cornerTR>
				<cornerBL>Damage/Corner</cornerBL>
				<cornerBR>Damage/Corner</cornerBR>
			</damageData>
		</graphicData>
		<castEdgeShadows>true</castEdgeShadows>
		<staticSunShadowHeight>0.20</staticSunShadowHeight>
		<costList>
			<Plasteel>350</Plasteel>
			<ComponentIndustrial>10</ComponentIndustrial>
		</costList>
		<altitudeLayer>Building</altitudeLayer>
		<fillPercent>0.5</fillPercent>
		<useHitPoints>True</useHitPoints>
		<statBases>
			<WorkToBuild>3000</WorkToBuild>
			<MaxHitPoints>500</MaxHitPoints>
			<Flammability>0</Flammability>
			<Cleanliness>0</Cleanliness>
			<Mass>200</Mass>
		</statBases>
		<size>(3,2)</size>
		<designationCategory>Production</designationCategory>
		<canOverlapZones>false</canOverlapZones>
		<minifiedDef>MinifiedThing</minifiedDef>
		<passability>PassThroughOnly</passability>
		<terrainAffordanceNeeded>Medium</terrainAffordanceNeeded>
		<pathCost>50</pathCost>
		<hasInteractionCell>True</hasInteractionCell>
		<interactionCellOffset>(0,0,-1)</interactionCellOffset>
		<surfaceType>Item</surfaceType>
		<constructionSkillPrerequisite>4</constructionSkillPrerequisite>
		<thingCategories>
			<li>BuildingsProduction</li>
		</thingCategories>
		<comps>
			<li Class="FS_SSR.Generic.TCP_ICMR" />
		</comps>
		<allowedRocketCapacity>6</allowedRocketCapacity>
	</ThingDef>
	
	<ThingDef Class="FS_SSR.Satellite.LaunchStationDef" ParentName="SSR_LaunchStation">
		<defName>SSR_SpaceElevator</defName>
		<label>太空电梯</label>
		<description>火箭发射台</description>
		<thingClass>FS_SSR.Satellite.Building_SpaceElevator</thingClass>
		<placeWorkers>
		    <li>FS_SSR.Generic.PW_WorldUniqueBuilding</li>
		</placeWorkers>
	</ThingDef>
  
    <ThingDef ParentName="BuildingBase">
    <defName>ssr_skyfire</defName>
    <label>ship chunk</label>
    <description>A chunk of a spacecraft. It seems to have been torn off either by uncontrolled re-entry forces or some kind of weapon.\n\nHaving landed not long ago, it still contains useful resources which you can acquire by deconstructing it.</description>
    <category>Building</category>
    <graphicData>
      <texPath>Things/Building/Exotic/ShipChunk</texPath>
      <graphicClass>Graphic_Random</graphicClass>
      <drawSize>(2,2)</drawSize>
      <damageData>
        <rect>(0.1,0.1,1.8,1.8)</rect>
      </damageData>
      <shadowData>
        <volume>(1.39,0.5,1.25)</volume>
      </shadowData>
    </graphicData>
    <altitudeLayer>Building</altitudeLayer>
    <pathCost>42</pathCost>
    <blockWind>true</blockWind>
    <passability>PassThroughOnly</passability>
    <fillPercent>0.50</fillPercent>
    <size>(2,2)</size>
    <statBases>
      <MaxHitPoints>300</MaxHitPoints>
      <Flammability>0</Flammability>
      <Beauty>-20</Beauty>
      <WorkToBuild>12000</WorkToBuild>
    </statBases>
    <leaveResourcesWhenKilled>false</leaveResourcesWhenKilled>
    <killedLeavings>
      <ChunkSlagSteel>2</ChunkSlagSteel>
      <ComponentIndustrial>1</ComponentIndustrial>
    </killedLeavings>
    <costList>
      <ComponentIndustrial>11</ComponentIndustrial>
      <Steel>40</Steel>
    </costList>
    <building>
      <claimable>false</claimable>
      <alwaysDeconstructible>true</alwaysDeconstructible>
    </building>
    <selectable>true</selectable>
    <soundImpactDefault>BulletImpact_Metal</soundImpactDefault>
    <drawerType>MapMeshOnly</drawerType>
    <repairEffect>ConstructMetal</repairEffect>
  </ThingDef>
  
</Defs>